<plugin name='multi-tenant-core' version='1.0.3' grailsVersion='1.3.0 &gt; *'>
  <author>Eric Martineau, Scott Ryan</author>
  <authorEmail>ericm@infusionsoft.com, scryan@codehaus.org</authorEmail>
  <title>Multi-Tenant Plugin(Core)</title>
  <description> Allows for managing data for mutiple 'tenants' in a single database by using a tenantId column
                          for each domain object.  Also handles the proxying of spring beans for a multi-tenant environment. </description>
  <documentation>http://grails.org/multi-tenant-core</documentation>
  <resources>
    <resource>BuildConfig</resource>
    <resource>TenantClassInjector</resource>
    <resource>TenantFilters</resource>
    <resource>UrlMappings</resource>
  </resources>
  <dependencies>
    <plugin name='falconeUtil' version='1.0' />
  </dependencies>
  <behavior />
</plugin>