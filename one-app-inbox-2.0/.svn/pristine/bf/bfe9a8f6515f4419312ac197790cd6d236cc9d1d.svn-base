.thumbStyle{
	cursor:pointer;
	display:block;
	float:left;
	height:20px;
	width:26px;
	
}

.ui-autocomplete-loading{
	backgroud: url("../images/ajax-loader.gif") no-repeat right;
}
.pay_now_button{
	background-image:url(/one-app-cloud/images/Pay_Now.png);
	background-size: 120px;
	background-position: center;
	overflow: hidden;
	width: 120px;
	height: 28px;
}
.itmHeader td{font-weight:bold}
table.itmTable{border: 0;width: 100%;margin-bottom: 5px;color:inherit;vertical-align: top;}
.itm{vertical-align: top;}
.itmTable td{padding:5px;line-height: 16px;text-align:left;border-bottom:1px dotted #D5D5D5}
.itmTable td.num{text-align:right;white-space:nowrap}
.itmTable img{border:1px dotted #D5D5D5}
.itmTable img:hover{box-shadow: 0px 0px 6px 3px #D4F5FA}
.itmTable td:last-child.num{font-weight:bold}
.itmTable td:first-child{padding-left:0px}
.itmTable td:nth-child(2){width:100%}
div.amt,div.t{display:inline}